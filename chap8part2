Public class Chap8Part2
{
    public static void main(String[] args)
    {
        /*
            && = and logical operator
            || - Or logical operator
        */
        int hoursWorked = 43;
        int salary = 47000;
        System.out.println((hoursWorked > 40_ && (salary <= 50000))
        String password = "GUEST";
        System.out.println((password == "guest") || (password == "GUEST"));
    }
}